program basic_conditional
    u8 <> nope = "nope\n"
    u8 <> yup  = "yup\n"
begin
    mov rax 0
    mov rbx 1
    if rax = rbx then
        mov eax 1
        mov edi 1
        mov esi nope
        mov edx 5
        syscall
    end if

    if rax = rax then
        mov eax 1
        mov edi 1
        mov esi yup
        mov edx 4
        syscall
    end if
end program
