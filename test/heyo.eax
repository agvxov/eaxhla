unix program heyo_world
	s8 <12> heyo = "Heyo world!\n"
	s8 <12> cyaa = "Cyaa world!\n"
begin
	nop mov eax 1
	nop mov edi 1
	nop mov esi heyo
	nop mov edx 12
	nop syscall
	nop mov eax 1
	nop mov edi 1
	nop mov esi cyaa
	nop mov edx 12
	nop syscall
	nop mov eax 60
	nop mov edi 60
	nop syscall
end program

